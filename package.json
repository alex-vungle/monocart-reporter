{
    "name": "monocart-reporter",
    "version": "1.1.0",
    "description": "A playwright test reporter",
    "main": "lib/index.js",
    "scripts": {
        "build": "sf lint && sf b -p",
        "patch": "npm run build && sf publish patch -r",
        "pack": "sf lint && sf pack -o docs -q v={version}",
        "test": "node ./scripts/link.js && npx playwright test --config=tests/playwright.config.js"
    },
    "workspaces": [
        "packages/*"
    ],
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cenfun/monocart-reporter.git"
    },
    "dependencies": {
        "@babel/code-frame": "^7.18.6",
        "eight-colors": "^1.0.2",
        "lz-utils": "^1.0.5",
        "marked": "^4.2.3",
        "stack-utils": "^2.0.6"
    },
    "devDependencies": {
        "@playwright/test": "^1.28.0",
        "ansi-to-html": "^0.7.2",
        "eslint": "^8.28.0",
        "eslint-config-plus": "^1.0.5",
        "eslint-plugin-html": "^7.1.0",
        "eslint-plugin-vue": "^9.7.0",
        "github-markdown-css": "^5.1.0",
        "playwright": "^1.28.0",
        "postcss": "^8.4.19",
        "postcss-html": "^1.5.0",
        "postcss-scss": "^4.0.5",
        "stylelint": "^14.15.0",
        "stylelint-config-recommended": "^9.0.0",
        "stylelint-config-standard": "^29.0.0",
        "turbogrid": "^3.0.3",
        "vine-ui": "^3.0.14"
    }
}